<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.mtq.zqydatastorage">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name=".app.DataApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/BaseTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.file.FileActivity">
        </activity>
        <activity android:name=".ui.activity.file.FilePathActivity">
        </activity>
        <activity android:name=".ui.activity.file.FileDataActivity">
        </activity>
        <activity android:name=".ui.activity.file.InternalStorageActivity">
        </activity>
        <activity android:name=".ui.activity.file.ExternalStorageActivity">
        </activity>
        <activity android:name=".ui.activity.sp.SharedPreferencesActivity">
        </activity>
        <activity android:name=".ui.activity.file.ApiActivity">
        </activity>
        <activity android:name=".ui.activity.file.ExclusiveActivity">
        </activity>
    </application>

</manifest>